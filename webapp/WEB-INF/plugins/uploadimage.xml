<?xml version="1.0" encoding="UTF-8" standalone="no"?><plug-in>
    <name>uploadimage</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>uploadimage.plugin.description</description>
    <provider>uploadimage.plugin.provider</provider>
    <provider-url>http://your.web.site.com</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2013 Your Company</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>UPLOADIMAGE_MANAGEMENT</feature-id>
            <feature-title>uploadimage.adminFeature.ManageUploadimage.name</feature-title>
            <feature-description>uploadimage.adminFeature.ManageUploadimage.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/uploadimage/ManageUploadimage.jsp</feature-url>
            <feature-icon-url/>
      </admin-feature>
    </admin-features>    

    <!-- XPages parameters -->
    <applications>
        <application>
            <application-id>uploadimage</application-id>
            <application-class>fr.paris.lutece.plugins.uploadimage.web.UploadimageApp</application-class>
        </application>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
   </portlets>
   
   <css-stylesheets>
        <css-stylesheets-scope>portal</css-stylesheets-scope>
        <css-stylesheet>uploadimage/cropper.css</css-stylesheet>
        <css-stylesheet>uploadimage/main.css</css-stylesheet>
    </css-stylesheets>
    
	<javascript-files>
        <javascript-files-scope>portal</javascript-files-scope>
        <javascript-file>uploadimage/cropper.js</javascript-file>
      
    </javascript-files>
   
   
</plug-in>